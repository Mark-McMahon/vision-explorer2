#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules/@bramus/specificity/bin/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules/@bramus/specificity/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules/@bramus/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules/@bramus/specificity/bin/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules/@bramus/specificity/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules/@bramus/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/@bramus+specificity@2.4.2/node_modules:/Users/markmcmahon/projects/MCPapps/vision-explorer2/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
